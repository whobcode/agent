{
  "name": "chat4",
  "main": "src/index.ts",
  "workers_dev": false,
  "compatibility_date": "2025-07-21",
  "compatibility_flags": ["nodejs_compat", "global_fetch_strictly_public"],
  "routes": [
    {
      "pattern": "https://chat4.hwmnbn.me/",
      "zone_name": "hwmnbn.me"
    },
    {
      "pattern": "http://chat4.hwmnbn.me/",
      "zone_name": "hwmnbn.me"
    }
  ],
  "placement": {
    "mode": "smart"
  },
  "migrations": [
    {
      "tag": "v3",
      "new_classes": [
        "MyAgent",
        "MCPAgent"
      ]      
    }
  ],
  "assets": {
    "directory": "public/",
    "binding": "ASSETS"
  },
  "ai": {
    "binding": "AI"
  },
  "vars": {
    "AI_GATEWAY_NAME": "moongate",
    "DEFAULT_MODEL_ID": "deepseek-v3.1",
    "DEFAULT_SYSTEM_PROMPT": "You are a helpful AI agent.",
    "AI_GATEWAY_ENDPOINT": "https://gateway.ai.cloudflare.com/v1/moongate",
    "AI_ACCOUNT_ID": "93e0a6dda0f48d77b387cae708297820 ",
    "STRIPE_PRICE_ID": "YOUR_STRIPE_PRICE_ID",
    "AUTORAG_PROJECT_NAME": "whoblevi"
  },
  "durable_objects": {
    "bindings": [
      {
        "name": "MyAgent",
        "class_name": "MyAgent"
      },
      {
        "name": "MCP",
        "class_name": "MCPAgent"
      }
    ]
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "chat-db",
      "database_id": "7e986ff1-1205-417b-84ae-883683e9e82e",
      "preview_database_id": "YOUR_D1_PREVIEW_DATABASE_ID"
    }
  ],
  "kv_namespaces": [
    {
      "binding": "SESSIONS",
      "id": "155713e242ad44fab31732e4dc53907a",
      "preview_id": "YOUR_KV_PREVIEW_NAMESPACE_ID"
    }
  ],
  "r2_buckets": [
    {
      "binding": "AGENT_BUCKET",
      "bucket_name": "agent-bucket",
      "preview_bucket_name": "agent-bucket-preview"
    }
  ],
  "browser": {
    "binding": "BROWSER"
  },
  "vectorize": [
    {
      "binding": "VECTORIZE_INDEX",
      "index_name": "agent-vectorize-index"
    }
  ]
}
